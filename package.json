{
  "name": "reo",
  "version": "0.0.4",
  "description": "inspired by dva",
  "main": "dist/reo.js",
  "scripts": {
    "dev": "onchange \"src\" -i -v -- npm run build",
    "build": "npm run build:reo && npm run build:logger",
    "build:reo": "bubleup ./src/index.js -d dist --format umd --name reo",
    "build:logger": "bubleup ./src/plugins/logger.js -d ./ --format umd --name logger",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fengzilong/reo.git"
  },
  "author": "fengzilong",
  "license": "MIT",
  "eslintConfig": {
    "extends": "mo/browser"
  },
  "bugs": {
    "url": "https://github.com/fengzilong/reo/issues"
  },
  "homepage": "https://github.com/fengzilong/reo#readme",
  "dependencies": {},
  "devDependencies": {
    "bubleup": "^0.4.3",
    "eslint": "^3.8.0",
    "eslint-config-mo": "latest",
    "eventemitter2": "^2.1.3",
    "onchange": "^3.0.2",
    "regular-router": "0.0.2",
    "regularjs": "^0.5.1"
  }
}
