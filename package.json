{
  "name": "reo",
  "version": "1.0.0",
  "description": "inspired by dva",
  "main": "dist/reo.js",
  "scripts": {
    "dev": "onchange \"src\" -i -v -- npm run build",
    "build": "npm run build:reo && npm run build:logger",
    "build:reo": "bili --out-dir dist --format umd --compress --map",
    "build:logger": "bili src/plugins/logger.js --out-dir ./ --format umd --module-name logger --filename logger",
    "test": "npm run lint",
    "lint": "eslint src"
  },
  "files": [
    "dist",
    "logger.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fengzilong/reo.git"
  },
  "author": "fengzilong",
  "license": "MIT",
  "eslintConfig": {
    "extends": "mo/browser"
  },
  "bugs": {
    "url": "https://github.com/fengzilong/reo/issues"
  },
  "homepage": "https://github.com/fengzilong/reo#readme",
  "dependencies": {},
  "devDependencies": {
    "bili": "^0.17.4",
    "dush": "^3.0.5",
    "eslint": "^3.8.0",
    "eslint-config-mo": "latest",
    "onchange": "^3.0.2",
    "regular-router": "^1.0.0",
    "regularjs": "^0.5.1"
  }
}
